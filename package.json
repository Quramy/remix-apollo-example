{
  "name": "remix-apollo-example",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#app/*": [
      "./src/app/*",
      "./src/app/*.js",
      "./src/app/*.jsx",
      "./src/app/*/index.jsx",
      "./src/app/*/index.js",
      "./src/app/*.ts",
      "./src/app/*.tsx",
      "./src/app/*/index.ts",
      "./src/app/*/index.tsx"
    ],
    "#support/*": [
      "./src/support/*",
      "./src/support/*.js",
      "./src/support/*.jsx",
      "./src/support/*/index.jsx",
      "./src/support/*/index.js",
      "./src/support/*.ts",
      "./src/support/*.tsx",
      "./src/support/*/index.tsx",
      "./src/support/*/index.ts"
    ]
  },
  "scripts": {
    "prepare": "npm run generate",
    "clean": "rimraf -g build \"src/**/__generated__/**\" \"src/app/gql/**/*\"",
    "prebuild": "npm run generate",
    "build": "react-router build",
    "generate": "npm run generate:schema && npm run generate:prisma && npm run generate:graphql",
    "generate:schema": "node --import=tsimp/import scripts/extractSchema.ts",
    "generate:prisma": "prisma generate",
    "generate:graphql": "npm run codegen",
    "dev": "cross-env NODE_ENV=development node server.mjs",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "prettier . --write",
    "start": "cross-env NODE_ENV=production node server.mjs",
    "migrate:dev": "cross-env DATABASE_NAME=dev prisma migrate dev",
    "migrate:dev:reset": "cross-env DATABASE_NAME=dev prisma migrate reset",
    "migrate:test": "cross-env DATABASE_NAME=test prisma migrate dev",
    "migrate:test:reset": "cross-env DATABASE_NAME=test prisma migrate reset",
    "seed": "cross-env DATABASE_NAME=dev node --import=tsimp/import scripts/seed.ts",
    "typecheck": "tsc",
    "codegen": "graphql-codegen-esm --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@react-router/express": "^7.0.0-pre.0",
    "@react-router/fs-routes": "^7.0.0-pre.0",
    "@react-router/node": "^7.0.0-pre.0",
    "@react-router/serve": "^7.0.0-pre.0",
    "@prisma/client": "^5.0.0",
    "graphql": "^16.6.0",
    "graphql-yoga": "^5.0.0",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.0.0-pre.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-document-nodes": "^4.0.9",
    "@graphql-codegen/typescript-resolvers": "4.2.1",
    "@ngneat/falso": "^7.0.0",
    "@quramy/jest-prisma-node": "^1.3.0",
    "@quramy/prisma-fabbrica": "^2.0.0",
    "@react-router/dev": "^7.0.0-pre.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prettier": "^2.8.0",
    "prisma": "^5.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.4",
    "ts-graphql-plugin": "^4.0.3",
    "tsimp": "^2.0.11",
    "typescript": "~5.5.0",
    "typescript-eslint-language-service": "^5.0.5",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
